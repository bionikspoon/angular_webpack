{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "globals": { },
  "extends": [ "airbnb/base", "angular" ],
  "parserOptions": { "sourceType": "module" },
  "rules": {
    "no-use-before-define": [ 2, { "functions": false } ],
    "array-bracket-spacing": [ 2, "always" ],
    "no-unused-vars": 1,
    "max-len": [ 1, 120 ],
    "angular/no-service-method": 0,
    "angular/log": 1
  }
}
